{
  "version": 2,
  "builds": [{
    "src": "main.py",
    "use": "@vercel/python@3.0.2",
    "config": {
      "maxLambdaSize": "25mb",
      "includeFiles": "main.py|requirements.txt",
      "zeroConfig": true,
      "bundle": false
    }
  }],
  "routes": [
    {
      "src": "/",
      "dest": "/",
      "headers": { "Cache-Control": "no-store" }
    },
    {
      "src": "/webhook",
      "dest": "/webhook",
      "methods": ["POST"]
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/404"
    }
  ]
}
