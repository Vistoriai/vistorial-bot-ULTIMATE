{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "25mb",
        "suppressLoaders": true,
        "includeFiles": "main.py|requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/",
      "methods": ["GET"]
    },
    {
      "src": "/webhook",
      "dest": "/webhook",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404"
    }
  ]
}
